# %% [markdown]
# variation of parameters

# %%
x=sp.symbols("x")
y1=1
y2=1
g=1
w=sp.simplify(sp.matrices.dense.wronskian([y1,y2],x))
Y=-y1*sp.integrate(y2*g/w,x)+y2*sp.integrate(y1*g/w,x)
sp.simplify(Y)
c1,c2=sp.symbols("c1 c2")
ans=c1*y1+c2*y2+Y
sp.simplify(ans)

